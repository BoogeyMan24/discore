{
	"name": "discore",
	"version": "0.0.1",
	"description": "Simple, flexible, and most importantly versatile all-in-one discord bot framework.",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/BoogeyMan24/Discore.git"
	},
	"keywords": [
		"discord",
		"discord.js",
		"discore"
	],
	"author": "BoogeyMan",
	"license": "ISC",
	"type": "module",
	"bugs": {
		"url": "https://github.com/BoogeyMan24/Discore/issues"
	},
	"homepage": "https://github.com/BoogeyMan24/Discore#readme",
	"packageManager": "pnpm@10.19.0",
	"devDependencies": {
		"@eslint/js": "^9.38.0",
		"@types/node": "^24.9.1",
		"eslint": "^9.38.0",
		"tsup": "^8.5.0",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"discord.js": "^14.24.0"
	}
}
