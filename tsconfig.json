{
	"compilerOptions": {
		"outDir": "./dist",
		"declaration": true,
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"removeComments": true,
		"noImplicitAny": true,
	},
	"include": ["src/**/*.ts"],
}